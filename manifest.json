{
  "manifest_version": 2,
  "name": "RegHi",
  "version":"0.0.0.1",
  "description": "Regular expression highlighter for chrome.",
  "icons":{
	"128":"icon128.png",
  	"48":"icon48.png",
	"16":"icon16.png"
  },
  "version": "1.0",
  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>"  
  ],
  "browser_action": {
    "default_title": "Make this page red",
    "default_popup": "popup.html"
  }
}
